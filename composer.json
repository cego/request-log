{
    "name": "cego/request-log",
    "description": "Package for laravel that enables the logging of all incoming requests",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@creationsftw.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-mbstring": "*",        
        "nesbot/carbon": "^2.17",
        "illuminate/http": "^11.0",
        "illuminate/support": "^11.0",
        "cego/elastic-apm-wrapper": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "doctrine/dbal": "^3.3",
        "cego/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cego\\RequestLog\\RequestLogServiceProvider"
            ]
        }
    }
}
